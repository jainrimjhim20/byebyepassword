<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="2.5" client="site" method="upgrade">
	<name>Bye-Bye Password</name>
	<author>Team Ready Bytes</author>
	<creationDate>September 2013</creationDate>
	<copyright>Copyright (C) 2013 - 2015 Open Source Matters. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later</license>
    <authorEmail>rimjhim@readybytes.in</authorEmail>
    <authorUrl>http://www.jpayplans.com</authorUrl>
	<version>1.0.4</version>
	<description></description>
	<files>
		<filename module="mod_byebyepassword">mod_byebyepassword.php</filename>
		<folder>tmpl</folder>
		<folder>language</folder>
		<filename>helper.php</filename>
		<filename>mod_byebyepassword.xml</filename>
	</files>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="login"
					type="menuitem"
					disable="separator"
					label="MOD_BBPASS_FIELD_LOGIN_REDIRECTURL_LABEL"
					description="MOD_BBPASS_FIELD_LOGIN_REDIRECTURL_DESC" >
					<option
						value="">JDEFAULT</option>
				</field>
				<field
					name="logout"
					type="menuitem"
					disable="separator"
					label="MOD_BBPASS_FIELD_LOGOUT_REDIRECTURL_LABEL"
					description="MOD_BBPASS_FIELD_LOGOUT_REDIRECTURL_DESC" >
					<option
						value="">JDEFAULT</option>
				</field>	
			</fieldset>
		</fields>
	</config>
</extension>
